#ifndef STD_FUNDAMENTAL_INLINE_SRX
#define STD_FUNDAMENTAL_INLINE_SRX

#include <fundamental/inline/group.srx>
#include <fundamental/inline/set.srx>
#include <fundamental/inline/regex.srx>

#define inline(CLASS_TYPE, CONTENT) \
__INLINE__(CLASS_TYPE)__START__ \
	contains: CONTENT; __INLINE_CR__ \
__END__

#define raw_inline(CLASS_TYPE, CONTENT) \
__INLINE__(CLASS_TYPE)__START__ \
	raw: CONTENT; __INLINE_CR__ \
__END__

#endif
